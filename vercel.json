{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "config/local": false,
      "config/remote": false
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "frontend/app/api/**/*.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 60
    }
  }
}